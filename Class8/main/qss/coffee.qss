QSlider#horizontalSlider_play::groove:horizontal {
	border: 1px solid #C0C0C0;
	background: #C0C0C0;
	height: 7px;
	border-radius: 2px;
	padding-left:-2px;
	padding-right:-2px;
	padding-top:-2px;
	padding-bottom:-2px;
}

QSlider#horizontalSlider_play::sub-page:horizontal {
	background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
	stop:0 rgb(31,156,207), stop:1 rgb(44,203,231));
	background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1,
	stop:0 rgb(31,156,207), stop: 1 rgb(44,203,231));
	border: 1px solid #4A708B;
	height: 10px;
	border-radius: 2px;
	border-color: rgb(92,132,43);
}

QSlider#horizontalSlider_play::add-page:horizontal {
	background: #575757;
	border: 0px solid #777;
	height: 10px;
	border-radius: 2px;
}

QSlider#horizontalSlider_play::handle:horizontal
{
	/*
	background-color: none;
	image: url(:/res/res/image/default/media_handle.png);
	*/
	background: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.5, fx:0.5, fy:0.5,
	stop:0.6 #1BA6CC, stop:0.778409 rgba(255, 255, 255, 255));
	width: 11px;
	margin-top: -3px;
	margin-bottom: -3px;
	border-radius: 5px;
	border-color: rgb(27,166,204);
}

