QSlider#slider_mediaProgress::groove:horizontal {
	border: 1px solid rgb(255,255,255,0);
	background: rgb(255,255,255,0);
	height: 6px;
	border-radius: 0px;
	padding-left:0px;
	padding-right:0px;
	padding-top:0px;
	padding-bottom:0px;
}

QSlider#slider_mediaProgress::sub-page:horizontal {
	background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1,
	stop:0 rgb(255,100,126), stop: 1 rgb(255,151,165));
	border: 1px solid rgb(255,100,126);
	height: 6px;
	border-radius: 3px;
	border-color: rgb(255,100,126);
}

QSlider#slider_mediaProgress::add-page:horizontal {
	background: rgb(164,188,196);
	border: 0px solid rgb(164,188,196);
	height: 6px;
	border-radius: 3px;
}

QSlider#slider_mediaProgress::handle:horizontal
{
	background:none;
	border-image: url(:/res/res/image/default/play_progress_handle.png);
	width: 16px;
	margin-top: -5px;
	margin-bottom: -5px;
	border-radius: 5px;
}
QSlider#slider_mediaProgress::sub-page:horizontal:disabled {
	background: rgb(192, 192, 192);
	border-color: rgb(192, 192, 192);
}

QSlider#slider_mediaProgress::add-page:horizontal:disabled {
	background: rgb(255, 255, 255);
	border-color: rgb(255, 255, 255);
}

